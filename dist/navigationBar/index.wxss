.navigation-bar {
  position: relative;
  left: 0;
  top: 0;
  width: 750rpx;
  color: #fff;
  background: #45b9e5;
  z-index: 999999999999;
  text-align: center;
  transition: 300ms;
}

.back {
  position: absolute;
  left: 0;
  top: 0;
  min-width: 70rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.icon {
  display: inline-block;
  font-family: 'iconfont' !important;
  speak: none;
  font-style: normal;
  font-weight: bold;
  font-variant: normal;
  text-transform: none;
  text-rendering: auto;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  vertical-align: middle;
  font-size: 38rpx;
}

.style-simple {
  display: flex;
  margin-left: 16rpx;
  align-items: center;
}
.icon-right:before {
  content: '\e600';
}
.icon-down:before {
  content: '\e601';
}
.style-simple .icon-fanhui:before {
  content: '\e642';
}

.style-simple .back-home {
  width: auto;
  text-align: center;
  font-size: 30rpx;
}

.black {
  /* background: #fff; */
  border-color: #e5e5e5 !important;
}

.white {
  background: rgba(0, 0, 0, 0.15);
  border-color: rgba(255, 255, 255, 0.25) !important;
}

.style-normal {
  border: 1px solid #ededed;
  border-radius: 32rpx;
  height: 64rpx;
  width: 176rpx;
  display: flex;
  margin-left: 16rpx;
  align-items: center;
}

.style-normal .icon-back {
  line-height: 64rpx;
  width: 87rpx;
  font-size: 44rpx;
}

.style-normal .icon-back:before {
  content: '\e614';
}

.style-normal .icon-home {
  width: 87rpx;
  text-align: center;
  font-size: 44rpx;
}

.style-normal .icon-home:before {
  content: '\e600';
}

.style-normal .split {
  border-left: 1rpx solid #e5e5e5;
  height: 32rpx;
  width: 1rpx;
}

.title {
  font-weight: bold;
}

.navigation-bar-holder {
  background: transparent;
  position: relative;
  z-index: 2;
}

@font-face {
  font-family: 'iconfont';
  src: url('//at.alicdn.com/t/font_2012419_0oc6bct2g5qj.eot?t=1597715998320'); /* IE9 */
  src: url('//at.alicdn.com/t/font_2012419_0oc6bct2g5qj.eot?t=1597715998320#iefix')
      format('embedded-opentype'),
    /* IE6-IE8 */
      url('data:application/x-font-woff2;charset=utf-8;base64,')
      format('woff2'),
    url('//at.alicdn.com/t/font_2012419_0oc6bct2g5qj.woff?t=1597715998320')
      format('woff'),
    url('//at.alicdn.com/t/font_2012419_0oc6bct2g5qj.ttf?t=1597715998320')
      format('truetype'),
    /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
      url('//at.alicdn.com/t/font_2012419_0oc6bct2g5qj.svg?t=1597715998320#iconfont')
      format('svg'); /* iOS 4.1- */
}
